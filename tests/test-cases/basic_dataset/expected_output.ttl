@prefix cnt: <http://www.w3.org/2011/content#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix geodcatap: <http://data.europa.eu/930/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

geodcatap: a dcterms:Standard .

<http://inspire.ec.europa.eu/metadata-codelist/ResponsiblePartyRole/pointOfContact> a dcat:Role .

<http://inspire.ec.europa.eu/metadata-codelist/ResponsiblePartyRole/publisher> a dcat:Role .

<https://example.com/dataset/sample-dataset-001> a foaf:Document ;
    dcterms:description "Direct download link for the sample dataset"@en ;
    dcterms:title "Download Sample Dataset"@en .

<http://publications.europa.eu/resource/authority/language/ENG> a dcterms:LinguisticSystem .

[] a dcat:Dataset ;
    geodcatap:resourceType <http://inspire.ec.europa.eu/metadata-codelist/ResourceType/dataset> ;
    geodcatap:topicCategory <http://inspire.ec.europa.eu/metadata-codelist/TopicCategory/environment> ;
    dcterms:created "2023-01-01"^^xsd:date ;
    dcterms:description "This is a sample dataset for testing the ISO 19139 to DCAT-AP transformation."@en ;
    dcterms:identifier "sample-dataset-001"^^xsd:string ;
    dcterms:language <http://publications.europa.eu/resource/authority/language/ENG> ;
    dcterms:provenance [ a dcterms:ProvenanceStatement ;
            dcterms:description "This is a test dataset created for validation purposes."@en ] ;
    dcterms:publisher [ a prov:Agent,
                foaf:Organization ;
            foaf:name "Data Provider Organization"@en ] ;
    dcterms:spatial [ a dcterms:Location ;
            dcat:bbox "{\"type\":\"Polygon\",\"coordinates\":[[[-10.0,60.0],[10.0,60.0],[10.0,40.0],[-10.0,40.0],[-10.0,60.0]]]}"^^geo:geoJSONLiteral,
                "<gml:Envelope srsName=\"http://www.opengis.net/def/crs/OGC/1.3/CRS84\"><gml:lowerCorner>-10.0 40.0</gml:lowerCorner><gml:upperCorner>10.0 60.0</gml:upperCorner></gml:Envelope>"^^geo:gmlLiteral,
                "POLYGON((-10.0 60.0,10.0 60.0,10.0 40.0,-10.0 40.0,-10.0 60.0))"^^geo:wktLiteral ] ;
    dcterms:title "Sample Dataset Title"@en ;
    dcat:keyword "dataset"@en,
        "sample"@en,
        "test"@en ;
    dcat:landingPage <https://example.com/dataset/sample-dataset-001> ;
    prov:qualifiedAttribution [ a prov:Attribution ;
            dcat:hadRole <http://inspire.ec.europa.eu/metadata-codelist/ResponsiblePartyRole/publisher> ;
            prov:agent [ a prov:Agent,
                        foaf:Organization ;
                    foaf:name "Data Provider Organization"@en ] ] ;
    foaf:isPrimaryTopicOf [ a dcat:CatalogRecord ;
            dcterms:conformsTo geodcatap: ;
            dcterms:identifier "basic-dataset-001"^^xsd:string ;
            dcterms:language <http://publications.europa.eu/resource/authority/language/ENG> ;
            dcterms:modified "2023-01-01"^^xsd:date ;
            dcterms:source [ a dcat:CatalogRecord ;
                    dcterms:conformsTo [ a dcterms:Standard ;
                            dcterms:title "ISO 19115:2003/19139"@en ;
                            owl:versionInfo "1.0"@en ] ;
                    dcterms:modified "2023-01-01"^^xsd:date ;
                    cnt:characterEncoding "UTF-8"^^xsd:string ] ;
            dcat:contactPoint [ a vcard:Organization ;
                    vcard:fn "Test Organization"@en ;
                    vcard:hasEmail <mailto:contact@test.org> ] ;
            prov:qualifiedAttribution [ a prov:Attribution ;
                    dcat:hadRole <http://inspire.ec.europa.eu/metadata-codelist/ResponsiblePartyRole/pointOfContact> ;
                    prov:agent [ a prov:Agent,
                                foaf:Organization ;
                            foaf:mbox <mailto:contact@test.org> ;
                            foaf:name "Test Organization"@en ] ] ] .

